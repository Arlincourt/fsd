include ../../components/title-3/index.pug

mixin range-slider(params)
  -
    const title = params.title || 'диапазон цены'
  .range-slider 
    .range-slider__header 
      +title-3(title) 
      p.range-slider__info 
    .range-slider__item
    input(class="range-slider__input" name="range-slider")